{
  "name": "arkipel-client",
  "version": "0.0.0",
  "description": "Client for Arkipel",
  "main": "index.js",
  "scripts": {
    "apollo-codegen": "npx apollo client:codegen --endpoint=http://local.arkipel.io:9192/query --target=typescript --excludes=node_modules/* --includes=**/*.tsx --outputFlat src/generated --useReadOnlyTypes",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --write \"**/*.{md,js,jsx,ts,tsx,json,css,scss}\"",
    "format-watch": "onchange \"**/*.{md,js,jsx,ts,tsx,json,css,scss}\" -- prettier --write {{changed}}",
    "start": "concurrently --kill-others \"webpack-dev-server --config webpack.dev.js --allowed-hosts local.arkipel.io\" \"npm:*-watch\"",
    "test-onchange": "onchange \"**/*.js\" \"**/*.jsx\" -- npm test"
  },
  "keywords": [
    "arkipel"
  ],
  "author": "Marc-Fran√ßois Cochaux-Laberge",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@types/hcaptcha__react-hcaptcha": "^0.1.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@apollo/client": "^3.0.2",
    "@hcaptcha/react-hcaptcha": "^0.2.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "graphql": "^15.3.0",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.8",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.2.0"
  }
}
