{
  "name": "arkipel-client",
  "version": "0.0.0",
  "description": "Client for Arkipel",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "format": "prettier --write \"**/*.{md,js,jsx,ts,tsx,json,css,scss}\"",
    "format-watch": "onchange \"**/*.{md,js,jsx,ts,tsx,json,css,scss}\" -- prettier --write {{changed}}",
    "start": "concurrently --kill-others \"webpack-dev-server --mode development --host 0.0.0.0\" \"npm:*-watch\"",
    "test-onchange": "onchange \"**/*.js\" \"**/*.jsx\" -- npm test"
  },
  "keywords": [
    "arkipel"
  ],
  "author": "Marc-Fran√ßois Cochaux-Laberge",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "apollo-boost": "^0.4.7",
    "graphql": "^15.0.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hcaptcha": "^0.1.1",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.1.2"
  }
}
