{
  "name": "arkipel-client",
  "version": "0.0.0",
  "description": "Client for Arkipel",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "format-watch": "onchange \"**/*.{js,jsx,ts,tsx,json,css,scss}\" -- prettier --write {{changed}}",
    "start": "concurrently --kill-others \"webpack-dev-server --mode development\" \"npm:*-watch\"",
    "test-onchange": "onchange \"**/*.js\" \"**/*.jsx\" -- npm test"
  },
  "keywords": [
    "arkipel"
  ],
  "author": "Marc-Fran√ßois Cochaux-Laberge",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "onchange": "^6.1.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2"
  }
}
